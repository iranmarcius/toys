<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE jfreechart SYSTEM "file:///d:/desenvolvimento/java/eclipse/Toys/dtd/jfreechart-1_0.dtd">
	
<jfreechart>
	<line>
		<legend>
			<paint>Color.BLUE</paint>
